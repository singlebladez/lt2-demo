-- Toggle chat
ChatMiscSec:Toggle("Toggle Chat", true, function(toggle)
if toggle then
game:GetService("StarterGui"):SetCoreGuiEnabled('Chat', true)
else
game:GetService("StarterGui"):SetCoreGuiEnabled('Chat', false)
end
end)
