				SelectedColor.Size = UDim2.new(0, 59, 0, 21)
				SelectedColor.AutoButtonColor = false
				SelectedColor.Font = Enum.Font.GothamMedium
				SelectedColor.Text = ""
				SelectedColor.TextColor3 = Color3.fromRGB(255, 255, 255)
