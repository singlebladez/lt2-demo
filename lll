-- Locate Players | Credits to "Real Panda" for their ESP library
PlayerThingsSec:Toggle("Locate Players", false, function(state)
   if state == true then
	   _G.WRDESPEnabled = true
   else
	   _G.WRDESPEnabled = false
   end
end)
